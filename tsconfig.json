{
	"compilerOptions": {
		"types": ["jest", "node"],
		"target": "es2016",
		"module": "commonjs",
		"moduleResolution": "node",
		"typeRoots": ["./types"],
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"skipLibCheck": true
	}
}
